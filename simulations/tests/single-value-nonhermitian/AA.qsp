// ../../../framework/build_framework/qsvt AA ./ qsvt-dyn
// ../../../framework/build_framework/qsvt AA ./ qsp   

// dt, nt_points
t_grid   0.5 80

// precision of the QSP circuit (precision of the resulting signal is ~10*eps)
eps      1e-6

// flag restart
flag_restart 0

// how to create the initial state (can be vector or oracle):
sel_init  vector   1

// whether to write description of the circuit gates to a .dat file
flag_print_gates_to_file false

// whether to print zero-ancilla states to console:
flag_print_zero_states true

// whether to print all states to console
flag_print_all_states false


