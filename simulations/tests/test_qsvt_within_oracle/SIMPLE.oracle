// ../../../framework/build_oracle/oracletool SIMPLE ./ 

CONSTANTS
	nx  2
END_CONSTANTS


OPTIONS
    sel_compute_output all
	sel_print_output   all // none, all, zero-ancillae
	flag_circuit 0 
	flag_tex     0
	tex_CL  10 
END_OPTIONS


CIRCUITS_DECLARATION 
	U_BE   2            aj 1 1 j <nx> 0 
	// U      4  cc 2 1 a_qsvt 1 1 aj 1 1 j <nx> 0  
	U      7 cc 2 1 a_qsvt 1 1 bb 2 1 aj 1 1 dd 1 1 j <nx> 0  ccc 1 1 
END_CIRCUITS_DECLARATION


MAIN_CIRCUIT   
	U        
	INPUT_STATE  j  0  end_register 
END_MAIN_CIRCUIT


CIRCUIT_STRUCTURE U_BE
	gate Rx aj 1 0.1 control  j 1 end_gate
	gate Rx  j 2 0.3 control aj 1 end_gate
	gate Rx  j 1   1 control  j 2 end_gate
END_CIRCUIT_STRUCTURE


CIRCUIT_STRUCTURE U 
    // gate QSVT  matrix-inversion  a_qsvt 1  U_BE 2  aj 1 j -1  end_gate
	// gate QSVT  gauss             a_qsvt 1  U_BE 2  aj 1 j -1  end_gate

	gate QSVT   gauss  a_qsvt 1  U_BE 2  aj 1 j -1  end_gate
	igate QSVT  gauss  a_qsvt 1  U_BE 2  aj 1 j -1  end_gate
			// ocontrol dd 1 end_gate
			// control cc 2 ocontrol ccc 1 control dd 1 ocontrol bb 2 end_gate
END_CIRCUIT_STRUCTURE





















 











