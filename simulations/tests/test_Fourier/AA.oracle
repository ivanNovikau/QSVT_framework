
// Launch from the current folder
// ../../../framework/build_oracle/oracletool AA ./ 


CONSTANTS
nq 4
END_CONSTANTS


OPTIONS
    sel_compute_output all
	sel_print_output   zero-ancillae  // none, all, zero-ancillae
	flag_circuit 0 
	flag_tex     0
	tex_CL  10 
END_OPTIONS


CIRCUITS_DECLARATION
A  2 c 2 1  r <nq> 0 
END_CIRCUITS_DECLARATION

INPUT_STATES   A        
STATE 2 c 0 r 3     end_register  
END_INPUT_STATES

CIRCUIT_STRUCTURE A
// igate Fourier r -1 ocontrol c 3 end_gate
gate Fourier r -1 end_gate
END_CIRCUIT_STRUCTURE



