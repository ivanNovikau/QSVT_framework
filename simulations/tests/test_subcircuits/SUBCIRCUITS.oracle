
// ../../../framework/build_oracle/oracletool SUBCIRCUITS ./



OPTIONS
    sel_compute_output all
	sel_print_output   all  // none, all, zero-ancillae
	flag_circuit 0 
	flag_tex     1
	tex_CL  10 
END_OPTIONS


CIRCUITS_DECLARATION
	A  1  x 4 0
	B  2  y 2 0 x 2 0 
END_CIRCUITS_DECLARATION

MAIN_CIRCUIT   A    
	INPUT_STATE  0
END_MAIN_CIRCUIT


CIRCUIT_STRUCTURE A
	gate X x 1 end_gate
	gate Y x 2 end_gate
	gate Z x 4 end_gate
	gate H x 8 end_gate
END_CIRCUIT_STRUCTURE


CIRCUIT_STRUCTURE B
	circuit A -1 end_circuit
	circuit A 2 y -1 x -1 end_circuit
	circuit A 2 x -1 y -1 end_circuit
END_CIRCUIT_STRUCTURE






















 











