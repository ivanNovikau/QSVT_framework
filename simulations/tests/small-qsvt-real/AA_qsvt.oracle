// ../../../framework/build_oracle/oracletool AA_qsvt ./ -flag_circuit 1 -flag_tex 1 -flag_compute_output 1 -flag_print_output  0  -tex_CL  10

// | 0, a |
// | b, 0 |


CONSTANTS
aa 2.532207e+00
ab 2.941258e+00  // non-hermitian
// ab 2.532207e+00     // hermitian
END_CONSTANTS


CIRCUITS_DECLARATION
OF  2        c 1 1 r 1 0 
OH  3  a 1 1 c 1 1 r 1 0 
OM  2        c 1 1 r 1 0 
U   3  a 1 1 c 1 1 r 1 0 
END_CIRCUITS_DECLARATION

INPUT_STATES   U         
STATE 1    r 1     end_register  
END_INPUT_STATES


CIRCUIT_STRUCTURE OF 
gate X  c 1 ocontrol r 1 end_gate
END_CIRCUIT_STRUCTURE


CIRCUIT_STRUCTURE OH
gate Ry a 1 <aa> control c 1 ocontrol r 1  end_gate
gate Ry a 1 <ab> control r 1 ocontrol c 1  end_gate
END_CIRCUIT_STRUCTURE


CIRCUIT_STRUCTURE OM
gate swap   c -1   r -1  end_gate
END_CIRCUIT_STRUCTURE



CIRCUIT_STRUCTURE U
circuit  OF 2 r -1 c -1       0 end_circuit
circuit  OH 3 r -1 c -1 a 1   0 end_circuit
circuit  OM 2 r -1 c -1       0 end_circuit
icircuit OF 2 r -1 c -1       0 end_circuit
END_CIRCUIT_STRUCTURE






















 











